<?xml version="1.0"?>
<config>
    <modules>
        <Collinsharper_Custom64Shipping>
            <version>0.0.1</version>
        </Collinsharper_Custom64Shipping>
    </modules>
    <frontend>
        <layout>
            <updates>
                <wayne_chcustom>
                    <file>wayne_chcustom.xml</file>
                </wayne_chcustom>
            </updates>
        </layout>
    </frontend>
	<admin>
		<routers>
			<wayne_chcustom>
				<use>admin</use>
				<args>
					<module>Collinsharper_Custom64Shipping</module>
					<frontName>custom</frontName>
				</args>
			</wayne_chcustom>
		</routers>
	</admin>
    <global>
        <rewrite>
            <wayne_order_edit>
                <from><![CDATA[#^/orderedit/order/*#]]></from>
                <to>/custom/order/</to>
            </wayne_order_edit>
        </rewrite>
        <helpers>
            <wayne_chcustom>
                <class>Collinsharper_Custom64Shipping_Helper</class>
            </wayne_chcustom>
            <orderedit>
				<rewrite>
					<reconcile>Collinsharper_Custom64Shipping_Helper_Data</reconcile>
				</rewrite>
			</orderedit>
        </helpers>
        <models>
	        <wayne_chcustom>
	             <class>Collinsharper_Custom64Shipping_Model</class>
	             <resourceModel>wayne_chcustom_resource</resourceModel>
	        </wayne_chcustom>
	        <wayne_chcustom_resource>
	        	<class>Collinsharper_Custom64Shipping_Model_Resource</class>
	        </wayne_chcustom_resource>
	        <orderedit>
	        	<rewrite>
	        		<edit_updater_type_eitems>Collinsharper_Custom64Shipping_Model_Eitems</edit_updater_type_eitems>
	        	</rewrite>
	        </orderedit>
	    </models>
    </global>
</config>
